<h1 class="index-page__header" >Users List</h1>

<% @users.each do |user| %>
  <div class="card">
    <table>
      <tr>
        <th>Username:</th>
        <td><%= user.username %></td>
      </tr>
      </tr>
      <tr>
        <th>Biography</th>
        <td><%= user.biography %></td>
      </tr>
      <tr>
        <th>Age</th>
        <td><%= user.age %></td>
      </tr>
      <tr>
        <th>Contacts</th>
        <td><%= user.contacts.count %></td>
      </tr>
      <tr>
        <th>Phone Numbers</th>
        <td><%= user.phone_numbers.count %></td>
    </table>
  </div>
<% end %>
